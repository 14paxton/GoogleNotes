							"    let flows = jsonData.flows.map(u => u?.name);\r",
							"\r",
							"    console.log(\"flows\", flows);\r",
							"        pm.expect(jsonData.flows.length).to.be.greaterThan(1);\r",
							"        pm.collectionVariables.set(\"flowList\", flows);\r",
							"    pm.globals.set(\"flowList\", flows);\r",
							"});\r",
							""
						],
